package Demo09;

public class MyThreadDemo {

	public static void main(String[] args) {
		MyThread mt1=new MyThread("A");
		MyThread mt2=new MyThread("B");
		MyThread mt3=new MyThread("C");
		
//		mt1.run();
//		mt2.run();
//		mt1.setName("线程一");
//		mt2.setName("线程二");
		mt1.start();
		mt1.join();
		mt2.start();
		mt3.start();
	}
}
