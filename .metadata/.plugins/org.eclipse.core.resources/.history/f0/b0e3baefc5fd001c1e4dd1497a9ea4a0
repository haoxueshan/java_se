package Demo05;

public class ArrayDemo {
	public static void main(String[] args) {
		int[] arr= {26,36,78,54};
		for(int i=0;i<arr.length;i++) {
			for(int x=0;x<arr.length-i;x++) {
				if(arr[x]<arr[x+1]) {
					int temp=arr[x];
					arr[x]=arr[x+1];
					arr[x+1]=temp;
				}
			}
		}
		System.out.println(arr.toString());
	}
}
