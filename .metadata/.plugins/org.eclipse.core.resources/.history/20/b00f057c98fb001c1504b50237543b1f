package hellow;

public class Demo074 {

	private String name;
	private int age;
	
	public Demo074() {
		System.out.println("无参构造方法");
	}
	
	public Demo074(String name) {
		this.name=name;
	}
	
	public Demo074(int age) {
		this.age=age;
	}
	public Demo074(String name,int age) {
		this.name=name;
		this.age=age;
	}
	
	public void show() {
		System.out.println(name+","+age);
	}
	
}
