package hellow;

public class Demo073 {

	private String name;
	private int age;
	
	public void setAge(int a) {
		
		
		if (a<0||a>120) {
			System.out.println("输入的年龄不对");
		}else {
			age=a;
		}
	}
	public int getAge() {
		return age;
	}
	public void steName(String n) {
		name=n;
	}
	public String getName() {
		return name;
	}
	public void show() {
		System.out.println("姓名："+name+" 年龄:"+age);
	}
}
